{
  "name": "anime-subtitle-translator",
  "version": "1.0.0",
  "description": "AI-powered anime subtitle translator supporting ASS and SRT formats",
  "main": "dist/main.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "start": "node --env-file .env dist/main.js",
    "check-types": "tsc --noEmit"
  },
  "keywords": [
    "anime",
    "subtitle",
    "translator",
    "ASS",
    "SRT",
    "AI"
  ],
  "author": "denizozaltay",
  "license": "GPL-3.0",
  "packageManager": "pnpm@10.27.0",
  "devDependencies": {
    "@types/node": "^25.0.5",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "axios": "^1.13.2"
  }
}
